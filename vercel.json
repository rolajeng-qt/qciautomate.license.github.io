{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate-for-app",
      "dest": "api/app.py"
    },
    {
      "src": "/api/get-hardware-id",
      "dest": "api/app.py"
    },
    {
      "src": "/api/create-default-license",
      "dest": "api/app.py"
    },
    {
      "src": "/api/validate-license",
      "dest": "api/app.py"
    },
    {
      "src": "/api/status",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}